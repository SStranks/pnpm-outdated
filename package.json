{
  "name": "pnpm-outdated",
  "version": "1.0.0",
  "description": "github action - runs `pnpm outdated` on repository and prints output to github summary",
  "author": "Simon Stranks <sstranks0@gmail.com> <https://github.com/SStranks>",
  "repository": {
    "type": "github",
    "url": "https://github.com/SStranks/pnpm-outdated"
  },
  "keywords": [
    "actions",
    "node",
    "outdated",
    "version",
    "semver"
  ],
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=20 <24"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error",
      "version": ">=20 <24"
    }
  },
  "prettier": "@packages/prettier-config",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "NODE_ENV=development node --import tsx src/main.ts",
    "build": "tsc ",
    "prepare": "husky",
    "lint": "eslint",
    "megalint": "docker run --rm -e MEGALINTER_CONFIG=.mega-linter.local.yaml -v \"$(pwd)\":/tmp/lint oxsecurity/megalinter:v8"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@packages/commitlint-config": "workspace:*",
    "@packages/eslint-config": "workspace:*",
    "@packages/prettier-config": "workspace:*",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@types/node": "^24.0.11",
    "@types/semver": "^7.7.0",
    "eslint": "9.30.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  }
}
